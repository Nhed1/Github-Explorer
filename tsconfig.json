{
  "compilerOptions": {
    "lib": ["dom", "DOM.Iterable", "esnext"],                                       
    "jsx": "react-jsx",                               
    "module": "commonjs",                               
    "moduleResolution": "node",                      
    "resolveJsonModule": true,                       
    "noEmit": true,                                  
    "isolatedModules": true,                         
    "allowSyntheticDefaultImports": true,            
    "esModuleInterop": true,                               
    "forceConsistentCasingInFileNames": true,           
    "strict": true,                                     
    "skipLibCheck": true,
    "allowJs": true,                                
  }, 
  "include": ["src"]
}
